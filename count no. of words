#include <stdio.h>
#include<string.h>
int main() {
    char s[20];
    int c=0,i;
    printf("enter the string");
    fgets(s,20,stdin);
    while(s[i]!='\0')
    {
        if(s[i]==' ')
        {
            c++;
        }
        i++;
    }
    c=c+1;
    printf(" no. of words in string %d",c);

    return 0;
