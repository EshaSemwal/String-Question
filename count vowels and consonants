#include <stdio.h>
#include<string.h>
int main() {
    char s[20],i;
    int cv=0,cc=0;
    printf("enter the string");
    fgets(s,20,stdin);
    while(s[i]!='\0')
    {
        if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u'||s[i]=='A' || s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U')
        {
            cv++;
        }
        else
        {
            cc++;
        }
        i++;
    }
    printf("%d\n",cv);
    printf("%d",cc);
    return 0;
}
