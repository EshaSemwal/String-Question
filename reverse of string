#include <stdio.h>
#include<string.h>
int main() {
    char s[20],t;
    int i,l=0,j;
    printf("enter the string");
    fgets(s,20,stdin);
    while (s[i]!='\0')
    {
        l++;
        i++;
    }
    
    for(i=0,j=l-1;i<j;i++,j--)
    {
        t=s[j];
        s[j]=s[i];
        s[i]=t;
    }
    printf("%s",s);

    return 0;
}
